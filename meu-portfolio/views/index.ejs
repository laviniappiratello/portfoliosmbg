    <section class="projetos-home">
      <h2 style="margin-bottom: 2%; margin-top: -20px;">Projetos Destacados</h2>
      <div class="cards">
        <% if (projetos && projetos.length) { %>
          <% projetos.forEach(projeto => { %>
            <div class="card" >
              <h3 style="display: flex; justify-content: center;" ><%= projeto.nome %></h3>
              <p style="display: flex; justify-content: center;" ><%= projeto.descricao %></p>
              <% if (projeto.github) { %>
                <div class="projeto-links" style="margin-top: 15px;">
                  <a href="<%= projeto.github %>" target="_blank" class="btn-github" >
                    <i class="fab fa-github"></i> Código no GitHub
                  </a>
                </div>
              <% } %>
            </div>
          <% }); %>
        <% } else { %>
          <p>Nenhum projeto disponível no momento.</p>
        <% } %>
        
